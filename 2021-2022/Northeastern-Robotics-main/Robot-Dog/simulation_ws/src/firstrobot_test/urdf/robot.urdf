<?xml version = "1.0"?>
<robot name = "my_robot" xmlns:xacro = "http://www.ros.org/wiki/xacro">
  <link name = "link_chassis">
    <pose> 0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="1"/>
      <origin rpy ="0 0 0" xyz="0 0 0"/>
      <inertia ixx = "0.1" ixy ="0" ixz = "0.1" iyz ="0" izz ="0.1"/>
    </inertial>

    <collision name ="collision_chassis">
      <origin rpy="0 0 0" xyz = "0 0 0" />
      <geometry>
        <box size = "1 1 1"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz = "0 0 0" />
      <geometry>
        <box size="1 1 1"/>
      </geometry>
    </visual>
  </link>
</robot>
